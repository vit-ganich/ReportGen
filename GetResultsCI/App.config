<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- New test results search configuration -->
    <!-- Files extension for search -->
    <add key="FilesExtension" value="trx" />
    <!-- Path to test results folder -->
    <add key="TestResultsDir" value="\\w10x64-29\Test Results\CI" />
    <!--add key="TestResultsDir" value="\\10.40.5.8\TestResults\CI\" /-->
    <!--To avoid the report encumbering, the number of errors must be limited /-->
    <add key="CountOfErrorsToInclude" value="5" />
    <!-- DateTime format for notifications -->
    <add key="DateTimeFormat" value="MM_dd_yyyy" />
    <!-- Test results file location -->
    <add key="ReportFolder" value="C:\CI_Reports" />
    <add key="ReportFileExtension" value="csv" />
    <!-- SMTP settings -->
    <!-- SMTP server, port (comma-separated, without spaces) -->
    <add key="SmtpServerPort" value="smtp.yandex.ru,587" />
    <!-- SSL enabled: true / false -->
    <add key="SmtpServerEnableSsl" value="true" />
    <!-- User email, password (comma-separated, without spaces)-->
    <add key="SmtpServerCredentials" value="test.testl@yandex.ru,password" />
    <!-- Sender email address -->
    <add key="SmtpMailFrom" value="test.test@yandex.ru" />
    <!-- Recipients adresses (comma-separated, without spaces) (or single address) -->
    <add key="SmtpMailTo" value="test@gmail.com" />
    <!-- Mail body -->
    <add key="SmtpMailBody" value="This is email with auto-generated CI summary" />
  </appSettings>
</configuration>