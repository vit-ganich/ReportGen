<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- New test results search configuration -->
    <!-- Files extension for search -->
    <add key="FilesExtension" value="trx" />
    <!-- Path to test results folder -->
    <add key="TestResultsDir" value="\\test\Test Results\CI" />
    <!--To avoid the report encumbering, the number of errors must be limited /-->
    <add key="CountOfErrorsToInclude" value="5" />
    <!-- DateTime format for notifications -->
    <add key="DateTimeFormat" value="MM_d_yyyy" />
    <!-- Test results file location -->
    <add key="ReportFolder" value="C:\CI_Reports" />
    <!-- Report file extension - csv or tsv. Excel formulas enabled only with TSV-->
    <add key="ReportFileExtension" value="tsv" />
    <!-- SMTP settings -->
    <!-- SMTP server, port (comma-separated, without spaces) -->
    <add key="SmtpServerPort" value="smtp.gmail.com,587" />
    <!-- SSL enabled: true / false -->
    <add key="SmtpServerEnableSsl" value="true" />
    <!-- User email, password (comma-separated, without spaces)-->
    <add key="SmtpServerCredentials" value="test@gmail.com,pass" />
    <!-- Recipients adresses (comma-separated, without spaces) (or single address) -->
    <add key="SmtpMailTo" value="test1@gmail.com" />
    <!-- Mail body -->
    <add key="SmtpMailBody" value="Auto-generated CI summary." />
  </appSettings>
</configuration>
